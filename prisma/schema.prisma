generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id           Int      @id @default(autoincrement()) @db.UnsignedInt
  name         String   @db.VarChar(200)
  is_active    Boolean
  created_date DateTime @db.DateTime(0)

  @@map("usuario")
}
